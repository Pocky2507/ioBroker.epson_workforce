            "Tinte",
            "Drucker",
            "pix",
            "office"
        ],
        "main": "main.js",
        "authors": [
            "rrov1"
        ],
        "license": "MIT",
        "readme": "https://github.com/rrov1/ioBroker.epson_workforce/blob/master/README.md",
        "icon": "epson_stylus_px830.png",
