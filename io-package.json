            },
            "native": {}
        },
        {
            "_id": "ip",
            "type": "state",
            "common": {
                "name": "Printer IP",
                "desc": "Printer IP adress",
                "type": "string",
                "read": true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "mac",
            "type": "state",
            "common": {
                "name": "Printer MAC",
                "desc": "Printer MAC adress",
                "type": "string",
                "read": true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "model",
            "type": "state",
            "common": {
                "name": "Printer Model",
                "desc": "Printer model type",
                "type": "string",
                "read": true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "name",
            "type": "state",
            "common": {
                "name": "Printer Name",
                "desc": "Printer Name",
                "type": "string",
                "read": true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "UNREACH",
            "type": "state",
            "common": {
                "name": "Printer not connected",
